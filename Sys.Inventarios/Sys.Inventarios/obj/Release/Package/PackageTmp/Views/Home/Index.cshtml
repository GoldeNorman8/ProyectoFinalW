@model Model.Usuarios
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-xl-8">
        <div class="c-card u-p-medium u-mb-medium">
            <div class="u-flex u-justify-between u-align-items-center">
                <h3 class="c-card__title">Graficas al Dia</h3>
                <span class="u-text-small u-text-uppercase u-text-mute">Noviembre 2018</span>
            </div>

            <canvas id="js-chart-revenue" width="300" height="178"></canvas>
        </div>
    </div>
    <div class="col-xl-4">
        <!-- Cards below rely on utilities (u-) as they are highly customizable -->
        <div class="c-card u-p-medium u-mb-small">
            <h4 class="u-h5">Clientes Activos</h4>
            <p class="u-text-small u-text-mute u-mb-small">Actualmente estas en el <strong class="u-text-bold">TOP 10%</strong> de la plataforma de inventarios <strong class="u-text-bold">bien</strong>.Sigue Con un gran trabajo! </p>
            <div class="o-line u-mb-small">
                <p class="u-text-small u-mr-small">Clasificacion</p>
                <div class="c-progress c-progress--success c-progress--small u-mb-zero">
                    <div class="c-progress__bar" style="width:72%;"></div>
                </div>
                <div class="u-text-small u-text-mute u-ml-small">72%</div>
            </div>
            <div class="o-line u-mb-small">
                <p class="u-text-small u-mr-small">Presipuesto</p>
                <div class="c-progress c-progress--fancy c-progress--small u-mb-zero">
                    <div class="c-progress__bar" style="width:42.6%;"></div>
                </div>
                <div class="u-text-small u-text-mute u-ml-small">42.6%</div>
            </div>
            <div class="o-line">
                <p class="u-text-small u-mr-small">Sincronizacion</p>
                <div class="c-progress c-progress--info c-progress--small u-mb-zero">
                    <div class="c-progress__bar" style="width:89.2%;"></div>
                </div>
                <div class="u-text-small u-text-mute u-ml-small">89.2%</div>
            </div>
        </div>
        <div class="c-card u-p-medium u-mb-small">
            <h4 class="u-h5">Tareas</h4>
            <div class="o-line u-mb-xsmall">
                <p class="u-text-small">Terminadas</p>
                <span class="u-text-small u-text-mute">340</span>
            </div>
            <div class="o-line u-mb-xsmall">
                <p class="u-text-small">Total</p>
                <span class="u-text-small u-text-mute">520</span>
            </div>
            <div class="o-line u-mb-xsmall">
                <p class="u-text-small">Promedio Mensual</p>
                <span class="u-text-small u-text-mute">2454</span>
            </div>
            <div class="c-divider u-mt-xsmall u-mb-xsmall"></div>
            <div class="o-line">
                <p class="u-text-small u-mr-small">Progreso</p>
                <div class="c-progress c-progress--success c-progress--small u-mb-zero">
                    <div class="c-progress__bar" style="width:65.3%;"></div>
                </div>
                <div class="u-text-small u-text-mute u-ml-small">65.3%</div>
            </div>
        </div>
    </div>
</div><!-- // .row -->
<div class="row">
    <div class="col-sm-12 col-lg-6 col-xl-3">
        <div class="c-state-card" data-mh="state-cards">
            <div class="c-state-card__icon c-state-card__icon--info">
                <i class="fa fa-line-chart"></i>
            </div>
            <div class="c-state-card__content">
                <h5 class="c-state-card__number">
                    Q.9,587
                    <span class="c-state-card__status u-bg-green">
                        <i class="fa fa-caret-up"></i>
                    </span>
                </h5>
                <p class="c-state-card__meta">Beneficio Total <span class="u-text-success">(+Q.286)</span></p>
            </div>
        </div>
    </div><!-- // .col-md-3 -->
    <div class="col-sm-12 col-lg-6 col-xl-3">
        <div class="c-state-card" data-mh="state-cards">
            <div class="c-state-card__icon c-state-card__icon--fancy">
                <i class="fa fa-pie-chart"></i>
            </div>
            <div class="c-state-card__content">
                <h5 class="c-state-card__number">
                    Q.2,190
                    <span class="c-state-card__status u-bg-green">
                        <i class="fa fa-caret-up"></i>
                    </span>
                </h5>
                <p class="c-state-card__meta">Ingresos <span class="u-text-success">(+Q.286)</span></p>
            </div>
        </div>
    </div><!-- // .col-md-3 -->
    <div class="col-sm-12 col-lg-6 col-xl-3">
        <div class="c-state-card" data-mh="state-cards">
            <div class="c-state-card__icon c-state-card__icon--warning">
                <i class="fa fa-user"></i>
            </div>
            <div class="c-state-card__content">
                <h5 class="c-state-card__number">
                    93
                    <span class="c-state-card__status u-bg-red">
                        <i class="fa fa-caret-down"></i>
                    </span>
                </h5>
                <p class="c-state-card__meta">Clientes Activos<span class="u-text-danger">(34)</span></p>
            </div>
        </div>
    </div><!-- // .col-md-3 -->
    <div class="col-sm-12 col-lg-6 col-xl-3">
        <div class="c-state-card" data-mh="state-cards">
            <div class="c-state-card__icon c-state-card__icon--success">
                <i class="fa fa-bar-chart"></i>
            </div>
            <div class="c-state-card__content">
                <h5 class="c-state-card__number">
                    Q.431
                    <span class="c-state-card__status u-bg-green">
                        <i class="fa fa-caret-up"></i>
                    </span>
                </h5>
                <p class="c-state-card__meta">Beneficio <span class="u-text-success">(+Q.3.11)</span></p>
            </div>
        </div>
    </div><!-- // .col-md-3 -->
</div><!-- // .row -->
<div class="row">
    <div class="col-lg-6">
        <div class="c-card c-card--responsive u-mb-medium">
            <div class="c-card__header c-card__header--transparent o-line">
                <h5 class="c-card__title">Visitas Recientes</h5>
                <div class="c-card__meta">
                    <a href="#">Ver Todas</a>
                </div>
            </div>
            

        </div>
    </div>
    <div class="col-lg-6">
        <div class="c-card c-card--responsive u-mb-medium">
            <div class="c-card__header c-card__header--transparent o-line">
                <h5 class="c-card__title">Mejores Clientes</h5>
                <a class="c-card__meta" href="#">Ver Todos</a>
            </div>
            

        </div>
    </div>
</div><!-- // .row -->
